{
  "name": "yml",
  "displayName": "yml",
  "publisher": "kazuwombat",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "yaml-X",
      "properties": {
        "yaml-X.targetDir": {
          "type": "string",
          "default": null,
          "description": "Target Yaml dir. Relative path from project root dir. ⚠️Do not need ./ e.g) src/locale"
        },
        "yaml-X.excludeTopKeyForCompletion": {
          "type": "boolean",
          "default": false,
          "description": "Ignore top key competion. It's useful if you use locale file which has top key like en.xxxx.yyyy \nand want completion like xxxx.yyyy without top key"
        },
        "yaml-X.yamlKeyArgFunctions": {
          "type": "array",
          "default": [],
          "description": "Function name of it use yaml key as argument. e.g) i18n.t"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "unit-test": "mocha 'out/test/unit/**/*.js'"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "yaml": "^2.0.0-4"
  }
}